{
  "name": "quotio-electron-go",
  "version": "0.1.0",
  "type": "module",
  "description": "Quotio - AI Provider Management for Coding Agents",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --config vite.config.electron.js && vite build --config vite.config.preload.js && electron-builder",
    "build:renderer": "vite build",
    "build:electron": "vite build --config vite.config.electron.js && vite build --config vite.config.preload.js",
    "preview": "vite preview",
    "electron:dev": "npm run build:electron && concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:pack": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "electron",
    "ai",
    "proxy",
    "quota"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^9.39.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@tanstack/react-router": "^1.70.0",
    "@tanstack/router-plugin": "^1.70.0",
    "axios": "^1.6.2",
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.6.0",
    "zustand": "^4.4.7"
  }
}
